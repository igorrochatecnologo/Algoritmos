algoritmo "ParImpar"
var
   N: Inteiro
   R: Caractere

funcao ParOuImpar (V: Inteiro): Caractere
inicio
    Se (V%2 = 0) entao
       Retorne "Par"
    senao
       Retorne "IMPAR"
    FimSe
Fimfuncao
inicio
      Escreva ("Digite um numero: ")
      Leia (N)
      R <- ParOuImpar (N)
      Escreva ("O numero ", N, " e um valor ", R)

fimalgoritmo
